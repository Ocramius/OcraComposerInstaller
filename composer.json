{
    "name": "ocramius/OcraComposerInstaller",
    "description": "Helper library that handles installation of ZendFramework modules with OcraComposer",
    "type": "zendframework-module-installer",
    "keywords": [
        "module",
        "zf2",
        "composer",
        "autoload",
        "installer"
    ],
    "homepage": "https://github.com/Ocramius/OcraComposerInstaller",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://marco-pivetta.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "psr-0": {
            "OcraComposerInstaller": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "extra": {
        "class": "OcraComposer\\Composer\\Installer"
    }
}
